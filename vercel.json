{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      },
      {
        "handle": "filesystem"
      },
        {
            "src": "/v1/status",
            "status": 200,
            "headers": {
            "content-type": "application/json"
            },
            "body": "{\"status\": \"ok\"}"
        }
    ]
  }